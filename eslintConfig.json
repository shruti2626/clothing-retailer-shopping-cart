{
	"extends": "eslint:recommended",
	"rules": {
		"no-alert": 0,
		"no-bitwise": 0,
		"camelcase": 2,
		"curly": 2,
		"eqeqeq": 2,
		"no-eq-null": 0,
		"guard-for-in": 1,
		"no-empty": 2,
        "no-use-before-define": 2,
        "no-obj-calls": 2,
        "no-unused-vars": 0,
        "new-cap": 2,
        "no-shadow": 0,
        "strict": 2,
		"no-empty-function":2,
		"no-multi-spaces":2,
		"no-loop-func":2,
        "no-invalid-regexp": 2,
        "comma-dangle": 2,
        "no-undef": 2,
        "no-new": 1,
        "no-extra-semi": 2,
        "no-debugger": 2,
        "no-caller": 1,
        "semi": 1,
        "quotes": 2,
        "no-unreachable": 2
	},
	"globals": { 
		"angular":false,
		"$":false,
		"consentApp":false,
		"_":false,
		"jQuery": true
	},
	"env": {
		"browser": true,
		"node": true
	}
}